{"ast":null,"code":"import store from '@/store';\nimport { mapGetters } from 'vuex';\nexport default {\n  name: \"ComplaintList\",\n  computed: {\n    ...mapGetters(['Token', 'UserInfo', 'RoleType', 'HasUserInfo', 'ColumnType', \"UserId\"])\n  },\n  data() {\n    return {\n      where: {\n        UserId: store.getters.UserId\n      },\n      // 投诉表单数据\n      complaintForm: {\n        Content: '',\n        UserId: store.getters.UserId,\n        ComplaintStatus: 1 // 默认状态为待处理\n      },\n      // 表单验证规则\n      complaintRules: {\n        Content: [{\n          required: true,\n          message: '请输入投诉内容',\n          trigger: 'blur'\n        }, {\n          min: 5,\n          message: '投诉内容至少5个字符',\n          trigger: 'blur'\n        }, {\n          max: 500,\n          message: '投诉内容最多500个字符',\n          trigger: 'blur'\n        }]\n      },\n      // 表格列定义\n      dataColum: [{\n        key: \"Id\",\n        title: \"流水编号\",\n        type: store.getters.ColumnType.SHORTTEXT,\n        width: 100\n      }, {\n        key: \"Content\",\n        title: \"投诉内容\",\n        type: store.getters.ColumnType.SHORTTEXT,\n        width: 200,\n        render: (h, {\n          row\n        }) => {\n          const content = row.Content || '';\n          const displayText = content.length > 50 ? content.substring(0, 50) + '...' : content;\n          return h('span', {\n            title: content\n          }, displayText);\n        }\n      }, {\n        key: \"AdminDto.Name\",\n        title: \"处理人\",\n        type: store.getters.ColumnType.SHORTTEXT,\n        width: 120,\n        render: (h, {\n          row\n        }) => {\n          return h('span', row.AdminDto?.Name || '待处理');\n        }\n      }, {\n        key: \"ComplaintStatusFormat\",\n        title: \"处理状态\",\n        type: store.getters.ColumnType.SHORTTEXT,\n        width: 100,\n        render: (h, {\n          row\n        }) => {\n          const status = row.ComplaintStatus;\n          const statusText = row.ComplaintStatusFormat || this.getStatusText(status);\n          const type = this.getStatusTagType(status);\n          return h('el-tag', {\n            props: {\n              type: type,\n              size: 'small'\n            }\n          }, statusText);\n        }\n      }, {\n        key: \"Response\",\n        title: \"处理回复\",\n        type: store.getters.ColumnType.SHORTTEXT,\n        width: 200,\n        render: (h, {\n          row\n        }) => {\n          const response = row.Response || '';\n          const displayText = response.length > 50 ? response.substring(0, 50) + '...' : response;\n          return h('span', {\n            title: response\n          }, displayText || '暂无回复');\n        }\n      }, {\n        key: \"CreateTime\",\n        title: \"提交时间\",\n        type: store.getters.ColumnType.DATETIME,\n        width: 160\n      }, {\n        key: \"operation\",\n        title: \"操作\",\n        type: store.getters.ColumnType.OPERATION,\n        width: 100,\n        slot: true\n      }],\n      // 当前查看的记录\n      currentRecord: {},\n      // 详情弹窗显示控制\n      detailDialogVisible: false,\n      // 提交状态\n      submitting: false\n    };\n  },\n  created() {\n    // 初始化时加载数据\n    this.$nextTick(() => {\n      if (this.$refs.PaginationTableId) {\n        this.$refs.PaginationTableId.Reload();\n      }\n    });\n  },\n  methods: {\n    // 返回上一个页面\n    goBack() {\n      this.$router.go(-1);\n    },\n    // 提交投诉\n    async submitComplaint() {\n      try {\n        const valid = await this.$refs.complaintFormRef.validate();\n        if (!valid) return;\n        this.submitting = true;\n\n        // 构建提交数据，使用后端枚举的默认值1（待处理）\n        const submitData = {\n          ...this.complaintForm,\n          ComplaintStatus: 1 // 对应枚举中的\"待处理\"\n        };\n        const {\n          Data\n        } = await this.$Post(\"/Complaint/CreateOrEdit\", submitData);\n        this.$message({\n          message: '投诉提交成功',\n          type: 'success'\n        });\n\n        // 重置表单\n        this.resetComplaintForm();\n\n        // 刷新表格数据\n        if (this.$refs.PaginationTableId) {\n          this.$refs.PaginationTableId.Reload();\n        }\n      } catch (error) {\n        console.error('提交投诉失败:', error);\n        this.$message.error('提交失败，请稍后重试');\n      } finally {\n        this.submitting = false;\n      }\n    },\n    // 重置投诉表单\n    resetComplaintForm() {\n      this.$refs.complaintFormRef.resetFields();\n      this.complaintForm = {\n        Content: '',\n        UserId: store.getters.UserId,\n        ComplaintStatus: 1 // 重置后仍保持待处理状态\n      };\n    },\n    // 刷新表格\n    refreshTable() {\n      if (this.$refs.PaginationTableId) {\n        this.$refs.PaginationTableId.Reload();\n      }\n    },\n    // 显示详情弹窗\n    showDetail(row) {\n      this.currentRecord = {\n        ...row\n      };\n      this.detailDialogVisible = true;\n    },\n    // 获取状态标签类型 - 根据后端枚举调整\n    getStatusTagType(status) {\n      // 根据提供的枚举类：\n      // 待处理(1) - 使用info类型（蓝色）\n      // 处理完成(2) - 使用success类型（绿色）\n      switch (status) {\n        case 1:\n          return 'info';\n        // 待处理\n        case 2:\n          return 'success';\n        // 处理完成\n        default:\n          return 'info';\n        // 默认\n      }\n    },\n    // 获取状态文本 - 根据后端枚举调整\n    getStatusText(status) {\n      const statusMap = {\n        1: '待处理',\n        2: '处理完成'\n      };\n      return statusMap[status] || '未知状态';\n    },\n    // 格式化日期\n    formatDate(date) {\n      if (!date) return '';\n      return new Date(date).toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n      }).replace(/\\//g, '-');\n    }\n  }\n};","map":{"version":3,"names":["store","mapGetters","name","computed","data","where","UserId","getters","complaintForm","Content","ComplaintStatus","complaintRules","required","message","trigger","min","max","dataColum","key","title","type","ColumnType","SHORTTEXT","width","render","h","row","content","displayText","length","substring","AdminDto","Name","status","statusText","ComplaintStatusFormat","getStatusText","getStatusTagType","props","size","response","Response","DATETIME","OPERATION","slot","currentRecord","detailDialogVisible","submitting","created","$nextTick","$refs","PaginationTableId","Reload","methods","goBack","$router","go","submitComplaint","valid","complaintFormRef","validate","submitData","Data","$Post","$message","resetComplaintForm","error","console","resetFields","refreshTable","showDetail","statusMap","formatDate","date","Date","toLocaleString","year","month","day","hour","minute","second","replace"],"sources":["src/views/Front/ComplaintList.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-page-header class=\"card margin-top-lg\" @back=\"goBack\" content=\"我的投诉\">\r\n    </el-page-header>\r\n\r\n    <!-- 投诉表单 -->\r\n    <el-card class=\"box-card margin-top-lg\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>填写投诉</span>\r\n      </div>\r\n      <div class=\"tb-body\">\r\n        <el-form ref=\"complaintFormRef\" :model=\"complaintForm\" :rules=\"complaintRules\" label-width=\"80px\">\r\n          <el-form-item label=\"投诉内容\" prop=\"Content\" required>\r\n            <el-input\r\n              v-model=\"complaintForm.Content\"\r\n              type=\"textarea\"\r\n              :rows=\"4\"\r\n              placeholder=\"请输入投诉内容...\"\r\n              maxlength=\"500\"\r\n              show-word-limit\r\n              clearable\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" size=\"medium\" @click=\"submitComplaint\" :loading=\"submitting\">\r\n              提交投诉\r\n            </el-button>\r\n            <el-button size=\"medium\" @click=\"resetComplaintForm\">重置</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </el-card>\r\n\r\n    <!-- 投诉记录列表 -->\r\n    <el-card class=\"box-card margin-top-lg\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>投诉记录</span>\r\n      </div>\r\n      \r\n      <PaginationTable \r\n        ref=\"PaginationTableId\" \r\n        url=\"/Complaint/List\" \r\n        :column=\"dataColum\" \r\n        :where=\"where\"\r\n      >\r\n        <template v-slot:header>\r\n          <el-button \r\n            type=\"text\" \r\n            icon=\"el-icon-refresh\" \r\n            @click=\"refreshTable\"\r\n          >\r\n            刷新\r\n          </el-button>\r\n        </template>\r\n        \r\n        <!-- 操作列插槽 -->\r\n        <template v-slot:operation=\"{ row }\">\r\n          <el-button \r\n            type=\"text\" \r\n            size=\"small\" \r\n            @click=\"showDetail(row)\"\r\n          >\r\n            查看详情\r\n          </el-button>\r\n        </template>\r\n      </PaginationTable>\r\n    </el-card>\r\n\r\n    <!-- 详情弹窗 -->\r\n    <el-dialog\r\n      :title=\"`投诉详情 - 流水号: ${currentRecord.Id || ''}`\"\r\n      :visible.sync=\"detailDialogVisible\"\r\n      width=\"600px\"\r\n      :close-on-click-modal=\"false\"\r\n    >\r\n      <div class=\"detail-content\">\r\n        <el-descriptions :column=\"1\" border>\r\n          <el-descriptions-item label=\"流水编号\">\r\n            {{ currentRecord.Id }}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"投诉内容\">\r\n            <div class=\"long-text\">{{ currentRecord.Content }}</div>\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"处理人\">\r\n            {{ currentRecord.AdminDto?.Name || '待处理' }}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"处理状态\">\r\n            <el-tag \r\n              :type=\"getStatusTagType(currentRecord.ComplaintStatus)\"\r\n              size=\"small\"\r\n            >\r\n              {{ currentRecord.ComplaintStatusFormat || getStatusText(currentRecord.ComplaintStatus) }}\r\n            </el-tag>\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"处理回复\">\r\n            <div class=\"long-text\">{{ currentRecord.Response || '暂无回复' }}</div>\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"提交时间\">\r\n            {{ formatDate(currentRecord.CreateTime) }}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"处理时间\" v-if=\"currentRecord.Response\">\r\n            {{ formatDate(currentRecord.UpdateTime) }}\r\n          </el-descriptions-item>\r\n        </el-descriptions>\r\n      </div>\r\n      \r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"detailDialogVisible = false\">关 闭</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport store from '@/store';\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  name: \"ComplaintList\",\r\n  computed: {\r\n    ...mapGetters([\r\n      'Token', 'UserInfo', 'RoleType', 'HasUserInfo', 'ColumnType', \"UserId\"\r\n    ])\r\n  },\r\n  data() {\r\n    return {\r\n      where: {\r\n        UserId: store.getters.UserId\r\n      },\r\n      // 投诉表单数据\r\n      complaintForm: {\r\n        Content: '',\r\n        UserId: store.getters.UserId,\r\n        ComplaintStatus: 1 // 默认状态为待处理\r\n      },\r\n      // 表单验证规则\r\n      complaintRules: {\r\n        Content: [\r\n          { required: true, message: '请输入投诉内容', trigger: 'blur' },\r\n          { min: 5, message: '投诉内容至少5个字符', trigger: 'blur' },\r\n          { max: 500, message: '投诉内容最多500个字符', trigger: 'blur' }\r\n        ]\r\n      },\r\n      // 表格列定义\r\n      dataColum: [\r\n        {\r\n          key: \"Id\",\r\n          title: \"流水编号\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n          width: 100\r\n        },\r\n        {\r\n          key: \"Content\",\r\n          title: \"投诉内容\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n          width: 200,\r\n          render: (h, { row }) => {\r\n            const content = row.Content || '';\r\n            const displayText = content.length > 50 ? content.substring(0, 50) + '...' : content;\r\n            return h('span', { title: content }, displayText);\r\n          }\r\n        },\r\n        {\r\n          key: \"AdminDto.Name\",\r\n          title: \"处理人\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n          width: 120,\r\n          render: (h, { row }) => {\r\n            return h('span', row.AdminDto?.Name || '待处理');\r\n          }\r\n        },\r\n        {\r\n          key: \"ComplaintStatusFormat\",\r\n          title: \"处理状态\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n          width: 100,\r\n          render: (h, { row }) => {\r\n            const status = row.ComplaintStatus;\r\n            const statusText = row.ComplaintStatusFormat || this.getStatusText(status);\r\n            const type = this.getStatusTagType(status);\r\n            return h('el-tag', {\r\n              props: {\r\n                type: type,\r\n                size: 'small'\r\n              }\r\n            }, statusText);\r\n          }\r\n        },\r\n        {\r\n          key: \"Response\",\r\n          title: \"处理回复\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n          width: 200,\r\n          render: (h, { row }) => {\r\n            const response = row.Response || '';\r\n            const displayText = response.length > 50 ? response.substring(0, 50) + '...' : response;\r\n            return h('span', { title: response }, displayText || '暂无回复');\r\n          }\r\n        },\r\n        {\r\n          key: \"CreateTime\",\r\n          title: \"提交时间\",\r\n          type: store.getters.ColumnType.DATETIME,\r\n          width: 160\r\n        },\r\n        {\r\n          key: \"operation\",\r\n          title: \"操作\",\r\n          type: store.getters.ColumnType.OPERATION,\r\n          width: 100,\r\n          slot: true\r\n        }\r\n      ],\r\n      // 当前查看的记录\r\n      currentRecord: {},\r\n      // 详情弹窗显示控制\r\n      detailDialogVisible: false,\r\n      // 提交状态\r\n      submitting: false\r\n    };\r\n  },\r\n  created() {\r\n    // 初始化时加载数据\r\n    this.$nextTick(() => {\r\n      if (this.$refs.PaginationTableId) {\r\n        this.$refs.PaginationTableId.Reload();\r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    // 返回上一个页面\r\n    goBack() {\r\n      this.$router.go(-1);\r\n    },\r\n    \r\n    // 提交投诉\r\n    async submitComplaint() {\r\n      try {\r\n        const valid = await this.$refs.complaintFormRef.validate();\r\n        if (!valid) return;\r\n        \r\n        this.submitting = true;\r\n        \r\n        // 构建提交数据，使用后端枚举的默认值1（待处理）\r\n        const submitData = {\r\n          ...this.complaintForm,\r\n          ComplaintStatus: 1 // 对应枚举中的\"待处理\"\r\n        };\r\n        \r\n        const { Data } = await this.$Post(\"/Complaint/CreateOrEdit\", submitData);\r\n        \r\n        this.$message({\r\n          message: '投诉提交成功',\r\n          type: 'success'\r\n        });\r\n        \r\n        // 重置表单\r\n        this.resetComplaintForm();\r\n        \r\n        // 刷新表格数据\r\n        if (this.$refs.PaginationTableId) {\r\n          this.$refs.PaginationTableId.Reload();\r\n        }\r\n        \r\n      } catch (error) {\r\n        console.error('提交投诉失败:', error);\r\n        this.$message.error('提交失败，请稍后重试');\r\n      } finally {\r\n        this.submitting = false;\r\n      }\r\n    },\r\n    \r\n    // 重置投诉表单\r\n    resetComplaintForm() {\r\n      this.$refs.complaintFormRef.resetFields();\r\n      this.complaintForm = {\r\n        Content: '',\r\n        UserId: store.getters.UserId,\r\n        ComplaintStatus: 1 // 重置后仍保持待处理状态\r\n      };\r\n    },\r\n    \r\n    // 刷新表格\r\n    refreshTable() {\r\n      if (this.$refs.PaginationTableId) {\r\n        this.$refs.PaginationTableId.Reload();\r\n      }\r\n    },\r\n    \r\n    // 显示详情弹窗\r\n    showDetail(row) 