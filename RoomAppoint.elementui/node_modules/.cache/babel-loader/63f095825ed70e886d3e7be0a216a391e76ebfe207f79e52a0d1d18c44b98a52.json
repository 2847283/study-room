{"ast":null,"code":"import store from '@/store';\nimport { mapGetters } from 'vuex';\nexport default {\n  name: \"ComplainList\",\n  computed: {\n    ...mapGetters(['Token', 'UserInfo', 'RoleType', 'HasUserInfo', 'ColumnType', \"UserId\"])\n  },\n  data() {\n    return {\n      // 投诉表单\n      complaintForm: {\n        content: ''\n      },\n      // 提交状态\n      submitting: false,\n      // 搜索表单\n      searchForm: {},\n      // 查询条件\n      where: {\n        UserId: store.getters.UserId // 只查询当前用户的投诉\n      },\n      // 表格列配置（与积分列表保持一致的方式）\n      dataColum: [{\n        key: \"Id\",\n        hidden: true\n      }, {\n        key: \"CreateTime\",\n        title: \"投诉时间\",\n        width: \"180\",\n        align: \"center\",\n        type: store.getters.ColumnType.SHORTTEXT\n      }, {\n        key: \"Content\",\n        title: \"投诉内容\",\n        minWidth: \"300\",\n        type: store.getters.ColumnType.SHORTTEXT,\n        formatter: row => {\n          return row.Content && row.Content.length > 50 ? row.Content.substring(0, 50) + '...' : row.Content;\n        }\n      }, {\n        key: \"UserDto.Name\",\n        title: \"投诉人\",\n        width: \"120\",\n        align: \"center\",\n        type: store.getters.ColumnType.SHORTTEXT\n      }, {\n        key: \"HandlerName\",\n        title: \"处理人\",\n        width: \"120\",\n        align: \"center\",\n        type: store.getters.ColumnType.SHORTTEXT,\n        formatter: row => {\n          return row.HandlerName || '未分配';\n        }\n      }, {\n        key: \"Status\",\n        title: \"处理状态\",\n        width: \"120\",\n        align: \"center\",\n        type: store.getters.ColumnType.CUSTOM,\n        render: (h, params) => {\n          const status = params.row.Status;\n          const statusText = this.getStatusText(status);\n          const statusType = this.getStatusType(status);\n          return h('el-tag', {\n            props: {\n              type: statusType,\n              size: 'small'\n            }\n          }, statusText);\n        }\n      }, {\n        key: \"action\",\n        title: \"操作\",\n        width: \"100\",\n        align: \"center\",\n        type: store.getters.ColumnType.CUSTOM,\n        render: (h, params) => {\n          return h('el-button', {\n            props: {\n              type: 'text',\n              size: 'small'\n            },\n            on: {\n              click: e => {\n                e.stopPropagation();\n                this.viewDetail(params.row);\n              }\n            }\n          }, '查看详情');\n        }\n      }],\n      // 详情弹窗\n      detailDialogVisible: false,\n      currentComplaint: {}\n    };\n  },\n  created() {\n    // 组件创建时不需要单独加载数据，PaginationTable会自动加载\n  },\n  methods: {\n    // 返回上一页\n    goBack() {\n      this.$router.go(-1);\n    },\n    // 提交投诉\n    async submitComplaint() {\n      if (!this.complaintForm.content.trim()) {\n        this.$message.warning('请输入投诉内容');\n        return;\n      }\n      this.submitting = true;\n      try {\n        // 调用后端API提交投诉（与积分数据获取方式一致）\n        const params = {\n          Content: this.complaintForm.content,\n          UserId: store.getters.UserId\n        };\n        const {\n          Success,\n          Message\n        } = await this.$Post(\"/Complaint/Add\", params);\n        if (Success) {\n          // 重置表单\n          this.resetForm();\n\n          // 刷新列表（与积分列表刷新方式一致）\n          this.$refs.PaginationTableId.Reload(this.searchForm);\n          this.$message.success('投诉提交成功！');\n        } else {\n          this.$message.error(Message || '提交失败');\n        }\n      } catch (error) {\n        console.error('提交失败:', error);\n        this.$message.error('提交失败，请重试');\n      } finally {\n        this.submitting = false;\n      }\n    },\n    // 重置表单\n    resetForm() {\n      this.complaintForm.content = '';\n    },\n    // 搜索按钮点击\n    async SearchClick() {\n      // 将搜索条件传递给PaginationTable组件重新加载（与积分列表一致）\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    // 重置搜索\n    async ResetClick() {\n      this.searchForm = {};\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n    },\n    // 刷新列表\n    async refreshList() {\n      this.$refs.PaginationTableId.Reload(this.searchForm);\n      this.$message.success('刷新成功');\n    },\n    // 查看详情\n    viewDetail(row) {\n      this.currentComplaint = {\n        ...row\n      };\n      this.detailDialogVisible = true;\n    },\n    // 根据状态值获取状态文本\n    getStatusText(status) {\n      const statusMap = {\n        '0': '待处理',\n        '1': '处理中',\n        '2': '已处理',\n        '3': '已关闭'\n      };\n      return statusMap[status] || '未知';\n    },\n    // 根据状态获取标签类型\n    getStatusType(status) {\n      const statusMap = {\n        '0': 'warning',\n        // 待处理\n        '1': 'info',\n        // 处理中\n        '2': 'success',\n        // 已处理\n        '3': 'danger' // 已关闭\n      };\n      return statusMap[status] || 'info';\n    },\n    // 导出投诉记录\n    async exportComplaints() {\n      try {\n        const {\n          Success,\n          Message,\n          Data\n        } = await this.$Post(\"/Complaint/Export\", {\n          ...this.searchForm,\n          UserId: store.getters.UserId\n        });\n        if (Success && Data) {\n          // 创建下载链接\n          const link = document.createElement('a');\n          link.href = Data.FileUrl || Data;\n          link.download = `投诉记录_${new Date().toLocaleDateString()}.xlsx`;\n          document.body.appendChild(link);\n          link.click();\n          document.body.removeChild(link);\n          this.$message.success('导出成功');\n        } else {\n          this.$message.error(Message || '导出失败');\n        }\n      } catch (error) {\n        console.error('导出失败:', error);\n        this.$message.error('导出失败，请重试');\n      }\n    }\n  }\n};","map":{"version":3,"names":["store","mapGetters","name","computed","data","complaintForm","content","submitting","searchForm","where","UserId","getters","dataColum","key","hidden","title","width","align","type","ColumnType","SHORTTEXT","minWidth","formatter","row","Content","length","substring","HandlerName","CUSTOM","render","h","params","status","Status","statusText","getStatusText","statusType","getStatusType","props","size","on","click","e","stopPropagation","viewDetail","detailDialogVisible","currentComplaint","created","methods","goBack","$router","go","submitComplaint","trim","$message","warning","Success","Message","$Post","resetForm","$refs","PaginationTableId","Reload","success","error","console","SearchClick","ResetClick","refreshList","statusMap","exportComplaints","Data","link","document","createElement","href","FileUrl","download","Date","toLocaleDateString","body","appendChild","removeChild"],"sources":["src/views/Front/ComplaintList.vue"],"sourcesContent":["<template>\r\n  <div class=\"complaint-container\">\r\n    <!-- 页面头部 -->\r\n    <el-page-header \r\n      class=\"page-header margin-top-lg\" \r\n      @back=\"goBack\" \r\n      content=\"投诉建议\"\r\n    ></el-page-header>\r\n\r\n    <!-- 投诉输入区域 -->\r\n    <el-card class=\"input-card margin-top-lg\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>我要投诉</span>\r\n      </div>\r\n      <el-form :model=\"complaintForm\" ref=\"complaintFormRef\">\r\n        <el-form-item>\r\n          <el-input\r\n            type=\"textarea\"\r\n            :rows=\"4\"\r\n            v-model=\"complaintForm.content\"\r\n            placeholder=\"请输入您的投诉内容...\"\r\n            maxlength=\"500\"\r\n            show-word-limit\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button \r\n            type=\"primary\" \r\n            @click=\"submitComplaint\"\r\n            :loading=\"submitting\"\r\n          >提交投诉</el-button>\r\n          <el-button @click=\"resetForm\">清空</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n\r\n    <!-- 搜索区域 -->\r\n    <el-card class=\"box-card margin-top-lg\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-search\" @click=\"SearchClick\">搜 索</el-button>\r\n        <el-button type=\"warning\" size=\"mini\" @click=\"ResetClick\" icon=\"el-icon-s-promotion\">重 置</el-button>\r\n        <el-button \r\n          type=\"info\" \r\n          size=\"mini\" \r\n          icon=\"el-icon-refresh\" \r\n          @click=\"refreshList\"\r\n          style=\"float: right;\"\r\n        >刷 新</el-button>\r\n      </div>\r\n      <div class=\"tb-body\">\r\n        <el-form ref=\"searchFormRef\" :model=\"searchForm\" :inline=\"true\" label-width=\"100px\" :style=\"`flex:1;`\">\r\n          <el-form-item label=\"投诉内容\" prop=\"ContentLike\">\r\n            <el-input v-model.trim=\"searchForm.ContentLike\" placeholder=\"请输入投诉内容\" :clearable=\"true\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"处理状态\" prop=\"Status\">\r\n            <el-select v-model=\"searchForm.Status\" placeholder=\"请选择状态\" clearable>\r\n              <el-option label=\"待处理\" value=\"0\"></el-option>\r\n              <el-option label=\"处理中\" value=\"1\"></el-option>\r\n              <el-option label=\"已处理\" value=\"2\"></el-option>\r\n              <el-option label=\"已关闭\" value=\"3\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"处理人\" prop=\"HandlerNameLike\">\r\n            <el-input v-model.trim=\"searchForm.HandlerNameLike\" placeholder=\"请输入处理人\" :clearable=\"true\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </el-card>\r\n\r\n    <!-- 投诉记录列表（使用PaginationTable组件） -->\r\n    <PaginationTable \r\n      ref=\"PaginationTableId\" \r\n      url=\"/Complaint/List\" \r\n      :column=\"dataColum\" \r\n      :where=\"where\"\r\n    >\r\n      <template v-slot:header>\r\n        <el-button \r\n          type=\"primary\" \r\n          size=\"mini\" \r\n          plain \r\n          icon=\"el-icon-download\"\r\n          @click=\"exportComplaints\"\r\n        >导出投诉记录</el-button>\r\n      </template>\r\n    </PaginationTable>\r\n\r\n    <!-- 投诉详情弹窗 -->\r\n    <el-dialog\r\n      title=\"投诉详情\"\r\n      :visible.sync=\"detailDialogVisible\"\r\n      width=\"600px\"\r\n      center\r\n    >\r\n      <div class=\"complaint-detail\">\r\n        <el-descriptions :column=\"1\" border>\r\n          <el-descriptions-item label=\"投诉时间\">\r\n            {{ currentComplaint.CreateTime }}\r\n          </el-descriptions-item>\r\n          \r\n          <el-descriptions-item label=\"投诉内容\">\r\n            <div class=\"detail-content\">\r\n              {{ currentComplaint.Content }}\r\n            </div>\r\n          </el-descriptions-item>\r\n          \r\n          <el-descriptions-item label=\"投诉人\">\r\n            <span>{{ currentComplaint.UserDto?.Name || '匿名用户' }}</span>\r\n          </el-descriptions-item>\r\n          \r\n          <el-descriptions-item label=\"处理人\">\r\n            <span v-if=\"currentComplaint.HandlerName\">\r\n              {{ currentComplaint.HandlerName }}\r\n            </span>\r\n            <span v-else style=\"color: #909399;\">未分配</span>\r\n          </el-descriptions-item>\r\n          \r\n          <el-descriptions-item label=\"处理状态\">\r\n            <el-tag \r\n              :type=\"getStatusType(currentComplaint.Status)\"\r\n              size=\"medium\"\r\n            >\r\n              {{ getStatusText(currentComplaint.Status) }}\r\n            </el-tag>\r\n          </el-descriptions-item>\r\n          \r\n          <el-descriptions-item \r\n            label=\"处理结果\" \r\n            v-if=\"currentComplaint.Result\"\r\n          >\r\n            <div class=\"detail-result\">\r\n              {{ currentComplaint.Result }}\r\n            </div>\r\n          </el-descriptions-item>\r\n          \r\n          <el-descriptions-item \r\n            label=\"处理时间\" \r\n            v-if=\"currentComplaint.HandleTime\"\r\n          >\r\n            {{ currentComplaint.HandleTime }}\r\n          </el-descriptions-item>\r\n        </el-descriptions>\r\n      </div>\r\n      \r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"detailDialogVisible = false\">关 闭</el-button>\r\n        <el-button \r\n          type=\"primary\" \r\n          @click=\"detailDialogVisible = false\"\r\n        >确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport store from '@/store';\r\nimport { mapGetters } from 'vuex'\r\n\r\nexport default {\r\n  name: \"ComplainList\",\r\n  computed: {\r\n    ...mapGetters([\r\n      'Token', 'UserInfo', 'RoleType', 'HasUserInfo', 'ColumnType', \"UserId\"\r\n    ])\r\n  },\r\n  data() {\r\n    return {\r\n      // 投诉表单\r\n      complaintForm: {\r\n        content: ''\r\n      },\r\n      \r\n      // 提交状态\r\n      submitting: false,\r\n      \r\n      // 搜索表单\r\n      searchForm: {},\r\n      \r\n      // 查询条件\r\n      where: {\r\n        UserId: store.getters.UserId  // 只查询当前用户的投诉\r\n      },\r\n      \r\n      // 表格列配置（与积分列表保持一致的方式）\r\n      dataColum: [\r\n        {\r\n          key: \"Id\",\r\n          hidden: true,\r\n        },\r\n        {\r\n          key: \"CreateTime\",\r\n          title: \"投诉时间\",\r\n          width: \"180\",\r\n          align: \"center\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"Content\",\r\n          title: \"投诉内容\",\r\n          minWidth: \"300\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n          formatter: (row) => {\r\n            return row.Content && row.Content.length > 50 \r\n              ? row.Content.substring(0, 50) + '...' \r\n              : row.Content;\r\n          }\r\n        },\r\n        {\r\n          key: \"UserDto.Name\",\r\n          title: \"投诉人\",\r\n          width: \"120\",\r\n          align: \"center\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n        },\r\n        {\r\n          key: \"HandlerName\",\r\n          title: \"处理人\",\r\n          width: \"120\",\r\n          align: \"center\",\r\n          type: store.getters.ColumnType.SHORTTEXT,\r\n          formatter: (row) => {\r\n            return row.HandlerName || '未分配';\r\n          }\r\n        },\r\n        {\r\n          key: \"Status\",\r\n          title: \"处理状态\",\r\n          width: \"120\",\r\n          align: \"center\",\r\n          type: store.getters.ColumnType.CUSTOM,\r\n          render: (h, params) => {\r\n            const status = params.row.Status;\r\n            const statusText = this.getStatusText(status);\r\n            const statusType = this.getStatusType(status);\r\n            \r\n            return h('el-tag', {\r\n              props: {\r\n                type: statusType,\r\n                size: 'small'\r\n              }\r\n            }, statusText);\r\n          }\r\n        },\r\n        {\r\n          key: \"action\",\r\n          title: \"操作\",\r\n          width: \"100\",\r\n          align: \"center\",\r\n          type: store.getters.ColumnType.CUSTOM,\r\n          render: (h, params) => {\r\n            return h('el-button', {\r\n              props: {\r\n                type: 'text',\r\n                size: 'small'\r\n              },\r\n              on: {\r\n                click: (e) => {\r\n                  e.stopPropagation();\r\n                  this.viewDetail(params.row);\r\n                }\r\n              }\r\n            }, '查看详情');\r\n          }\r\n        }\r\n      ],\r\n      \r\n      // 详情弹窗\r\n      detailDialogVisible: false,\r\n      currentComplaint: {}\r\n    };\r\n  },\r\n  created() {\r\n    // 组件创建时不需要单独加载数据，PaginationTable会自动加载\r\n  },\r\n  methods: {\r\n    // 返回上一页\r\n    goBack() {\r\n      this.$router.go(-1);\r\n    },\r\n    \r\n    // 提交投诉\r\n    async submitComplaint() {\r\n      if (!this.complaintForm.content.trim()) {\r\n        this.$message.warning('请输入投诉内容');\r\n        return;\r\n      }\r\n      \r\n      this.submitting = true;\r\n      \r\n      try {\r\n        // 调用后端API提交投诉（与积分数据获取方式一致）\r\n        const params = {\r\n          Content: this.complaintForm.content,\r\n          UserId: store.getters.UserId\r\n        };\r\n        \r\n        const { Success, Message } = await this.$Post(\"/Complaint/Add\", params);\r\n        \r\n        if (Success) {\r\n          // 重置表单\r\n          this.resetForm();\r\n          \r\n          // 刷新列表（与积分列表刷新方式一致）\r\n          this.$refs.PaginationTableId.Reload(this.searchForm);\r\n          \r\n          this.$message.success('投诉提交成功！');\r\n        } else {\r\n          this.$message.error(Message || '提交失败');\r\n        }\r\n        \r\n      } catch (error) {\r\n        console.error('提交失败:', error);\r\n        this.$message.error('提交失败，请重试');\r\n      } finally {\r\n        this.submitting = false;\r\n      }\r\n    },\r\n    \r\n    // 重置表单\r\n    resetForm() {\r\n      this.complaintForm.content = '';\r\n    },\r\n    \r\n    // 搜索按钮点击\r\n    async SearchClick() {\r\n      // 将搜索条件传递给PaginationTable组件重新加载（与积分列表一致）\r\n      this.$refs.PaginationTableId.Reload(this.searchForm);\r\n    },\r\n    \r\n    // 重置搜索\r\n    async ResetClick() {\r\n      this.searchForm = {};\r\n      this.$refs.PaginationTableId.Reload(this.searchForm);\r\n    },\r\n    \r\n    // 刷新列表\r\n    async refreshList() {\r\n      this.$refs.PaginationTableId.Reload(this.searchForm);\r\n      this.$message.success('刷新成功');\r\n    },\r\n    \r\n    // 查看详情\r\n    viewDetail(row) {\r\n      this.currentComplaint = { ...row };\r\n      this.detailDialogVisible = true;\r\n    },\r\n    \r\n    // 根据状态值获取状态文本\r\n    getStatusText(status) {\r\n      const statusMap = {\r\n        '0': '待处理',\r\n        '1': '处理中',\r\n        '2': '已处理',\r\n        '3': '已关闭'\r\n      };\r\n      return statusMap[status] || '未知';\r\n    },\r\n    \r\n    // 根据状态获取标签类型\r\n    getStatusType(status) {\r\n      const statusMap = {\r\n        '0': 'warning',  // 待处理\r\n        '1': 'info',     // 处理中\r\n        '2': 'success',  // 已处理\r\n        '3': 'danger'    // 已关闭\r\n      };\r\n      return statusMap[status] || 'info';\r\n    },\r\n    \r\n    // 导出投诉记录\r\n    async exportComplaints() {\r\n      try {\r\n        const { Success, Message, Data } = await this.$Post(\"/Complaint/Export\", {\r\n          ...this.searchForm,\r\n          UserId: store.getters.UserId\r\n        });\r\n        \r\n        if (Success && Data) {\r\n          // 创建下载链接\r\n          const link = document.createElement('a');\r\n          link.href = Data.FileUrl || Data;\r\n          link.download = `投诉记录_${new Date().toLocaleDateString()}.xlsx`;\r\n          document.body.appendChild(link);\r\n          link.click();\r\n          document.body.removeChild(link);\r\n          \r\n          this.$message.success('导出成功');\r\n        } else {\r\n          this.$message.error(Message || '导出失败');\r\n        }\r\n      } catch (error) {\r\n        console.error('导出失败:', error);\r\n        this.$message.error('导出失败，请重试');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.complaint-container {\r\n  padding: 20px;\r\n}\r\n\r\n.page-header {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.input-card,\r\n.box-card {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.detail-content,\r\n.detail-result {\r\n  line-height: 1.6;\r\n  padding: 8px;\r\n  background-color: #f5f7fa;\r\n  border-radius: 4px;\r\n  word-break: break-word;\r\n}\r\n\r\n.margin-top-lg {\r\n  margin-top: 16px;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n  display: table;\r\n  content: \"\";\r\n}\r\n.clearfix:after {\r\n  clear: both;\r\n}\r\n\r\n/* 表格行样式 */\r\n:deep(.pending-row) {\r\n  background-color: #fdf6ec !important;\r\n}\r\n\r\n:deep(.complaint-content) {\r\n  line-height: 1.5;\r\n}\r\n\r\n/* 响应式调整 */\r\n@media screen and (max-width: 768px) {\r\n  .complaint-container {\r\n    padding: 10px;\r\n  }\r\n  \r\n  :deep(.el-form-item) {\r\n    margin-bottom: 10px;\r\n  }\r